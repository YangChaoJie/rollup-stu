#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules:/mnt/d/study/YUI/node_modules:/mnt/d/study/node_modules:/mnt/d/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/tar@6.1.13/node_modules/tar/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/tar@6.1.13/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules:/mnt/d/study/YUI/node_modules:/mnt/d/study/node_modules:/mnt/d/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/tar@6.1.13/node_modules/tar/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/tar@6.1.13/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
