#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules/rollup/dist/bin/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules/rollup/dist/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules/rollup/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules:/mnt/d/study/YUI/node_modules:/mnt/d/study/node_modules:/mnt/d/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/rollup/dist/bin/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/rollup/dist/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/rollup/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules/rollup/dist/bin/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules/rollup/dist/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules/rollup/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/rollup@3.15.0/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/.pnpm/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules:/mnt/d/study/YUI/node_modules:/mnt/d/study/node_modules:/mnt/d/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/rollup/dist/bin/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/rollup/dist/node_modules:/mnt/d/study/YUI/rollup-stu/node_modules/rollup/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
